program best;
uses wincrt;
type
tab=array[1..20] of string;
var
t:tab;
procedure container(var t:tab);
var
borderra,contcolor,padgins:string;
begin
t[1]:='.container {border-radius: 40px;background-color: #F7BE81;padding: 5px;}';
writeln('donner le coleur de container');
readln(contcolor);
writeln('donner le padgin de la page');
readln(padgins);
writeln('donner le radius de border');
readln(borderra);
t[1]:='.container {border-radius:'+borderra+';background-color:'+contcolor+';padding:'+padgins+'px;}';
end;


begin

 container(t);


end.